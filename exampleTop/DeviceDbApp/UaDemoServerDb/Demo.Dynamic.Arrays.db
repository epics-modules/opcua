# Database connecting to the UaServerCpp example server
# that is part of all Unified Automation bundles

# Dynamic part of the Demo server

# waveform records connecting to arrays of different types

record(waveform, "$(P)$(R)wfbool") {
    field(DTYP, "OPCUA")
    field(INP,  "@$(SUBS) ns=2;s=Demo.Dynamic.Arrays.Boolean")
    field(FTVL, "UCHAR")
    field(NELM, "10")
    field(SCAN, "I/O Intr")
    field(TSE,  "-2")
    field(TPRO, "$(DEBUG=0)")
}

