# Database connecting to the UaServerCpp example server
# that is part of all Unified Automation bundles

# Dynamic part of the Demo server

# aai/aao records connecting to arrays of different types

record(aai, "$(P)$(R)aaichar") {
    field(DTYP, "OPCUA")
    field(INP,  "@$(SUBS) ns=2;s=Demo.Static.Arrays.SByte")
    field(FTVL, "CHAR")
    field(NELM, "10")
    field(SCAN, "I/O Intr")
    field(TSE,  "-2")
    field(TPRO, "$(DEBUG=0)")
}

record(aao, "$(P)$(R)aaochar") {
    field(DTYP, "OPCUA")
